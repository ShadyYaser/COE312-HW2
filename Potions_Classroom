package shady_main;

public class Potions_Classroom extends Location{

	static String name = "Potions Class";
	static String character = "Professor Snape";
	
	Potions_Classroom() {
		super(name, character);
	}

	@Override
	public void look() {
		System.out.println("The " + name + " is one of the core subjects that first year students are required to study. "
		+ character +" is here.");
	}
}
